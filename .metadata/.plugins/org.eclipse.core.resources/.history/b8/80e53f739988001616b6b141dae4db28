package lift;

public class LiftMain {
	public static void main(String[] args) {
		LiftView lv = new LiftView();
		lv.drawLift(0,3);
		lv.drawLevel(5,4);
		try {
			Thread.sleep(1000);
		} catch(InterruptedException e) { }
		lv.moveLift(0,1);
		lv.drawLift(1,2);
		lv.moveLift(1,2);
		lv.drawLift(2,1);
	}
}
